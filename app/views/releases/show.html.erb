<p id="notice"><%= notice %></p>

<p>
  <strong>Id:</strong>
  <%= @release.id %>
</p>

<p>
  <strong>Sha:</strong>
  <%= @release.sha %>
</p>

<p>
  <strong>Release date:</strong>
  <%= @release.release_date %>
</p>

<ul>
  <% @release.pull_request_commits.each do |commit| %>
  <li>
    <p>
      <strong>SHA:</strong>
      <%= commit.sha %>
    </p>

    <p>
      <strong>Description:</strong>
      <%= commit.description %>
    </p>

    <p>
      <strong>Author:</strong>
      <%= commit.author %>
    </p>

    <%= link_to 'Edit', edit_commit_path(commit) %> |
    <%= link_to 'Back', commits_path %>
  </li>
  <% end %>
</ul>

<%= link_to 'Edit', edit_release_path(@release) %> |
<%= link_to 'Back', releases_path %>
